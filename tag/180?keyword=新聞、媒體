		gtag('js', new Date());
		gtag('config', 'UA-111780472-1');
	</script>	<script nonce="6c4373436c35447446546e46346e696d">
		$().ready(function(){
			$("#search_submit").click(function(){
				if(!$("#keyword").val()){
					alert("請輸入『搜尋關鍵字』");
					$("#keyword").focus();
					return false;
				}
				
				$("form").submit();	
			})
			
			$("#keyword").keyup(function(e){
				if(e.keyCode == 13){
					if(!$(this).val()){
						alert("請輸入『搜尋關鍵字』");
						$("#keyword").focus();
						return false;
					}
				}
				$("form").submit();	
			})
		})	
	</script>
