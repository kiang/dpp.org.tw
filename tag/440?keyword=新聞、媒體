		$().ready(function(){
			$("#search_submit").click(function(){
				if(!$("#keyword").val()){
					alert("請輸入『搜尋關鍵字』");
					$("#keyword").focus();
					return false;
				}
				
				$("form").submit();	
			})
			
			$("#keyword").keyup(function(e){
				if(e.keyCode == 13){
					if(!$(this).val()){
						alert("請輸入『搜尋關鍵字』");
						$("#keyword").focus();
						return false;
					}
				}
				$("form").submit();	
			})
		})	
	</script>
