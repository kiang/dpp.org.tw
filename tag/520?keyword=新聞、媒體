				}
				if(!r.test($("#pay_money").val())){
					alert("『捐款金額』請輸入整數位數");
					$("#pay_money").focus();
					return false;
				}
				if($("#pay_money").val() && $("#pay_money").val() > 0){
					$("#pay_submit").show();
				}else{
					$("#pay_submit").hide();
				}
			})
			
			$(".donate_notice_a").click(function(){
				if($("#pay_money").val() && $("#pay_money").val() > 99){
					if(!r.test($("#pay_money").val())){
						$("#pay_submit").hide();
					}else{
						$("#pay_submit").show();
					}
				}else{
					$("#pay_submit").hide();
				}
			})
			
			$("#pay_submit").click(function(){
				if(!$("#pay_money").val())
				{
					alert("請輸入『捐款金額』")
					$("#pay_money").focus()
					return false;
				}
				if($("#pay_money").val() <= 99)
				{
					alert("『捐款金額』最低限額為 新台幣 100 元整")
					$("#pay_money").focus()
					return false;
				}
				window.open('https://www.dpp.org.tw/payments/ecpay/'+$("#pay_money").val(),'_blank');
			});
