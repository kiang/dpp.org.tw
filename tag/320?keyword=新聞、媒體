					alert("『捐款金額』請輸入整數位數");
					$("#pay_money").focus();
					return false;
				}
				if($("#pay_money").val() && $("#pay_money").val() > 0){
					$("#pay_submit").show();
				}else{
					$("#pay_submit").hide();
				}
			})
			
			$(".donate_notice_a").click(function(){
				if($("#pay_money").val() && $("#pay_money").val() > 99){
