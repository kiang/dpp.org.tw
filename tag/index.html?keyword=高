			$(".donate_money").click(function(){
				var donate_money = $(this).attr("value");
				$("#pay_money").val(donate_money);
				$("#donate_submit").trigger("click");
				
				if(donate_money){
					$("#pay_submit").show();
				}else{
					$("#pay_submit").hide();
				}
