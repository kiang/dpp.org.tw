		gtag('config', 'UA-111780472-1');
	</script>	<script nonce="73526e5848484b6c625235396c636b77">
		$().ready(function(){
			$("#search_submit").click(function(){
				if(!$("#keyword").val()){
					alert("請輸入『搜尋關鍵字』");
					$("#keyword").focus();
					return false;
				}
				
				$("form").submit();	
			})
			
			$("#keyword").keyup(function(e){
				if(e.keyCode == 13){
					if(!$(this).val()){
						alert("請輸入『搜尋關鍵字』");
						$("#keyword").focus();
						return false;
					}
				}
				$("form").submit();	
			})
		})	
	</script>
