			$("#media_contents").find("center").children("div").css("height","auto");
			$("#media_contents").find("center").children("div").children("iframe").css("height","auto");
			$("#media_contents").find("center").children("div").children("iframe").css("min-height","400px");
			
			$("#media_contents").find("a").each(function(){
				var local = $(this).attr('href');
				if(local.indexOf('www.dpp.org') == -1){
					$(this).attr('target','_blank');
				}
			})
			
			$("#media_contents").find("a").attr('target','_blank');
			$("#media_contents").find("a").css('text-decoration','underline');
			$("#media_contents").find("a").css('color','blue');
		})
