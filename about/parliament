			ajax_parliament_data($(".mainName").eq(0).children("li").eq(0).children("a").attr("ids"));
			$(".mainName").eq(0).children("li").eq(0).addClass("active");
			
			$(".parliament_style").click(function(){
				$(".parliament_style").parent("li").removeClass("selected");
				$(".parliament_style").children(".par_count").css("color","#ffda2d");
				$(this).parent("li").addClass("selected");
				$(this).children(".par_count").css("color","#3e997e");
				
				var area_ids = $(this).parent("li").attr("ids");
				
				$(".mainName").each(function(e){
					$(this).children("li").each(function(i){
						if(area_ids == $(this).children("a").attr("ids")){
							$(".mainName").children("li").removeClass("active");
							$(this).addClass("active");
							$(".membersLink").children("div").removeClass("active");
							$(this).parent("ul").parent("div").addClass("active");
							ajax_parliament_data($(this).children("a").attr("ids"));

						}
					})
				})
				
				lg = parseFloat($(this).parent("li").attr("lg"));
				lt = parseFloat($(this).parent("li").attr("lt"));
				map.panTo({lat: lg, lng: lt});
			})
			
			$(".mainName > li > a").click(function(){
				ajax_parliament_data($(this).attr("ids"));
				
				$(".map_list").removeClass("selected");
				$(".map_list").css("z-index","");
				
				area_id = "#map_div_"+$(this).attr("ids");
				$(area_id).addClass("selected");
				$(area_id).css("z-index","99");
				
				lg = parseFloat($(this).attr("lg"));
				lt = parseFloat($(this).attr("lt"));
				map.panTo({lat: lg, lng: lt});
				
			})
			
			$(".membersLink > div > h5").click(function(){
				ajax_parliament_data($(this).parent("div").children("ul").children("li").eq(0).children("a").attr("ids"));
				var area_ids = $(this).parent("div").children("ul").children("li").eq(0).children("a").attr("ids");
				
				$(".parliament_style").parent("li").each(function(){
					if(area_ids == $(this).attr("ids")){
						$(".parliament_style").parent("li").removeClass("selected");
						$(this).addClass("selected");
						
						$(".parliament_style").parent("li").css("z-index","");
						$(this).css("z-index","99");
						
						lg = parseFloat($(this).attr("lg"));
						lt = parseFloat($(this).attr("lt"));
						map.panTo({lat: lg, lng: lt});
					}
				})
			})
			
			$("#parliament_name").keyup(function(e){
				if($(this).val() && e.keyCode == 13){
					ajax_parliament_name_data($(this).val());
				}
			})
			
			function ajax_parliament_data(area_id){
				$.ajax({
					url: 'https://www.dpp.org.tw/about/ajax_parliament_data',
					type: "POST",
					data: {area_id:area_id},
					error: function(err){
						console.log(err);
					},
					success: function(result){
						$(".subName").html(result);
					}
				});
			}
			
			function ajax_parliament_name_data(name){
				$.ajax({
					url: 'https://www.dpp.org.tw/about/ajax_parliament_name_data',
					type: "POST",
					data: {name:name},
					dataType:'json',
					error: function(err){
						console.log(err);
					},
					success: function(result){
						if(result){
							if(result["cate_type"] == 2){
// 								console.log(result["parliament_map_area"]);
								$(".mainName > li > a").each(function(){
									if($(this).attr("ids") == result["parliament_map_area"]){
										$(".mainName").parent("div").removeClass("active");
										$(this).parent("li").parent("ul").parent("div").addClass("active");
										$(this).trigger("click");
									}
								})
							} else if(result["cate_type"] == 1){
								console.log(result["id"]);
								$(".membersLink > div > h5").each(function(){
									if($(this).attr("ids") == result["id"]){
										$(this).trigger("click");
									}
								})
							}
						}else{
							alert("查無資料");
						}
					}
				});
			}
		});
	</script>
</body>
<!-- InstanceEnd -->
</html>