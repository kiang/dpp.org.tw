		}
		
		function ajax_cp_district(city_id,post_id){
			var token_val = $('input[name="ci_csrf_token"]').eq(0).val();
			var url_text = "https://www.dpp.org.tw/signup/ajax_district"+"/"+city_id+"/"+post_id;
